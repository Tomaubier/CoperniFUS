@article{Genoves2023,
  title = {Variation in the Viscoelastic Properties of Polydimethylsiloxane ({{PDMS}}) with the Temperature at Ultrasonic Frequencies},
  author = {Genov{\'e}s, V. and Maini, L. and Roman, C. and Hierold, C. and Cesarovic, N.},
  year = {2023},

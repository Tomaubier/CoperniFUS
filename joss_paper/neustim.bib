@article{Estrada2018,
  title = {Virtual Craniotomy for High-Resolution Optoacoustic Brain Microscopy},
  author = {Estrada, H{\'e}ctor and Huang, Xiao and Rebling, Johannes and Zwack, Michael and Gottschalk, Sven and Razansky, Daniel},

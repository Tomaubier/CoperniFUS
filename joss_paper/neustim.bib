@inproceedings{Yen2020,
  title = {Active {{Damping}} of {{Air-backed Ultrasonic Transducers Using Arbitrary Waveform Generators}}},
  booktitle = {2020 {{IEEE International Ultrasonics Symposium}} ({{IUS}})},

@article{Marangell2007,
  title = {Neurostimulation Therapies in Depression: A Review of New Modalities},
  shorttitle = {Neurostimulation Therapies in Depression},
  author = {Marangell, L. B. and Martinez, M. and Jurdi, R. A. and Zboyan, H.},

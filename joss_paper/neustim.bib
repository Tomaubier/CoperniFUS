@incollection{Lallouette2013,
  title = {Topology {{Drives Calcium Wave Propagation}} in {{3D Astrocyte Networks}}},
  booktitle = {Proceedings of the {{European Conference}} on {{Complex Systems}} 2012},
  author = {Lallouette, Jules and Berry, Hugues},
